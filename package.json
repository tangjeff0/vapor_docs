{
  "name": "vapor-docs",
  "version": "1.0.0",
  "description": "A feature-rich collaborative text editor focused on a e s t h e t i c s",
  "build": {
    "appId": "com.electron.808069",
    "productName": "V A P O R D O C S",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main.js"
    ],
    "mac": {
      "category": "public.app-category.Application"
    },
    "linux": {
      "category": "Office"
    },
    "win": {
      "icon": "build/icon.ico"
    }
  },
  "homepage": "http://tangjeff0.github.io/vapor_docs",
  "main": "main.js",
  "scripts": {
    "predeploy": "npm run webpack",
    "deploy": "gh-pages -d build",
    "devFront": "concurrently -k \"npm run devWebpack\" \"npm run devApp\"",
    "devWebpack": "webpack-dev-server --content-base build/",
    "devApp": "electron main.js --dev",
    "webpack": "webpack",
    "start": "electron main.js",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/andywong418/vapour_docs",
  "keywords": [
    "aesthetics",
    "a e s t h e t i c",
    "vaporwave",
    "yung lean",
    "biohacking"
  ],
  "author": "tangsauce",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.55.2",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "gh-pages": "^2.2.0",
    "loglevel": "^1.4.1",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.6.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "draft-js": "^0.10.4",
    "findandreplacedomtext": "^0.4.5",
    "immutable": "^3.8.2",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.16",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "underscore": "^1.8.3"
  }
}
